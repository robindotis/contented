    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    {% if metadata.clacksOverhead %}<meta http-equiv="X-Clacks-Overhead" content="{{ metadata.clacksOverhead }}">{% endif %}
    <title>{{ metadata.title }} {{titleprefix}}{% if titleprefix %}{{ " " }}{% endif %}{% if title %}{{ title }}{% endif %}</title>

    <meta name="description" content="{{ metadata.description }}">
    {% if tags %}<meta name="keywords" content="{% for tag in tags %}{{ tag }},{% endfor %}" />{% endif %}

    <meta name="robots" content="index, follow, max-snippet:-1, max-image-preview:large, max-video-preview:-1">

    <link rel="canonical" href="{{ metadata.url }}{{ permalink }}">
    <link rel="me" href="{{ metadata.relMe }}">
    <link rel="alternate" type="application/rss+xml" title="{{ metadata.title }} RSS" href="/feed/">
    <link rel="alternate" href="{{ metadata.feed.path }}" type="application/atom+xml" title="{{ metadata.title }}">
    <link rel="alternate" href="{{ metadata.jsonfeed.path }}" type="application/json" title="{{ metadata.title }}">
    <link rel="icon" href="/assets/images/cw.jpg" type="image/jpeg" sizes="50x50">

